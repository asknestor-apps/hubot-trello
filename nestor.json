{
  "name": "Trello",
  "permalink": "trello",
  "description": "manage your Trello from Nestor",
  "environment_keys": {
    "NESTOR_TRELLO_KEY": {
      "required": true,
      "mode": "oauth",
      "default": "1b1de3d324adf3ea1ae5cddafe3e4888"
    },
    "NESTOR_TRELLO_TOKEN": {
      "required": true,
      "mode": "oauth",
      "extract_from": "token"
    },
    "NESTOR_TRELLO_BOARD": {
      "required": true,
      "mode": "user"
    }
  }
}
